<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta property="og:type" content="(1)ページタイプ" />
  <meta property="og:title" content="(2)ページタイトル" />
  <meta property="og:description" content="(3)ページ説明文" />
  <meta property="og:url" content="(4)ページURL" />
  <meta property="og:site_name" content="FGORankGenerator" />
  <meta property="og:image" content="(7)画像URL" />
  <!-- Twitterカードの設定 -->
  <meta name="twitter:card" content="summary" />
  <meta name="twitter:site" content="@@gunjo2000" />
  <title>@ViewData["Title"] - FGO RankGenerator</title>
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="icon" href="~/favicon.ico">
  @* analytics *@
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-ZPB84Z3MWC"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());
    gtag('config', 'G-ZPB84Z3MWC');
  </script>
  @* AdSense *@
  <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-5077226600373691" crossorigin="anonymous"></script>
</head>
<body>
  <header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
      <div class="container-fluid">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">FGO RankGenerator</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
          <ul class="navbar-nav flex-grow-1">
            <li class="nav-item">
              <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
            </li>
            <li class="nav-item d-flex align-items-center">
              <a href="https://twitter.com/share?ref_src=twsrc%5Etfw" class="twitter-share-button" data-text="FGOサーヴァントランキングを見ながら所持チェック！ - FGO RankGenerator" data-url="https://fgorankgenerator.azurewebsites.net/" data-hashtags="FGORankGenerator,FGO" data-show-count="false">Tweet</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>
  <div class="container">
    <main role="main" class="pb-3">
      @RenderBody()
    </main>
  </div>

  <footer class="border-top footer text-muted">
    <div class="container d-flex align-items-center">
      &copy; 2024 - FGO RankGenerator - <a asp-area="" asp-controller="Home" asp-action="Contact"> Twitter</a>
      <a href="https://twitter.com/share?ref_src=twsrc%5Etfw" class="twitter-share-button" data-text="FGOサーヴァントランキングを見ながら所持チェック！ - FGO RankGenerator" data-url="https://fgorankgenerator.azurewebsites.net/" data-hashtags="FGORankGenerator,FGO" data-show-count="false">Tweet</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
    </div>
  </footer>
  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

  @*ソート機能*@
  <script src="~/js/sort.js" asp-append-version="true"></script>
  @*カスタムJS*@
  <script src="~/js/site.js" asp-append-version="true"></script>

  @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
